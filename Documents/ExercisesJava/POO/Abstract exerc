abstract class Animal {

  public abstract void animalSound();
  public abstract void animalSize();
  public abstract void animalColor();

  public void sleep() {
    System.out.println("zzz");
  }
}

class Bear extends Animal {
  public void animalSound() {
    System.out.println("Bear Sound: Whroooar");
  }
  public void animalSize(){
    System.out.println("Bear Size: 1.90m");
  }
  public void animalColor(){
    System.out.println("Bear Color: Brown.");
  }

}

class Main {

  public static void main(String[] args) {
    Bear bear = new Bear();

    bear.animalSound();
    bear.animalColor();
    bear.animalSize();
    bear.sleep();
    
  }

}