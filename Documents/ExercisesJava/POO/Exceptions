///////////check age /////////////////// 


class Main {
  static void checkAge(int age){
    if (age < 18){
      throw new ArithmeticException("Access Denied - You must be at least 18 years old");
    } else {
      System.out.println("Accres Granted - You're old enough");
    }
  }

  public static void main(String[] args){
  checkAge(15);
  }
}

