public class Main {
  public static void main(String[] args){
    HashMap<String, Integer> people = new HashMap<String, Integer>();
    people.put("John", 32);
    people.put("Stevie", 30);
    people.put("Angie", 33);

    for (String i : people.keySet()){
      System.out.println("key: " + i + " valeu: " + people.get(i));
    }
  }
}